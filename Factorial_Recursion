package Factorial;

public class Factorial {

    public static int fact(int n){
        if (n==0){
            return 1;
        }
        return n*fact(n-1);
    }

    public static int fibonnaci(int n){
        if(n==0 || n==1){
            return n;
        }
        return fibonnaci(n-1)+fibonnaci(n-2);
    }

}
